+            } catch (IllegalStateException e) {
+                success = false;
+                closeIdlingDatabase();
+                announceConnection();
+                db = getDatabase();
+            }
+        }
+        if (success == false) 
+        	throw new Exception("query - failed to execute query on the DB");
+        return c;
+    }
