+            PrivacyDebugger.e(TAG, "purgeSettings - purging DB failed.", e);
+            result = false;
+        } finally {
+            if (c != null) c.close();
+            watchDog.onEndAuthorizedTransaction();
+            mLock.writeLock().unlock();
+            closeIdlingDatabase();
+        }
